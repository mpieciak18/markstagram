{
  "name": "@markstagram/server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "pnpm -w test:server:docker",
    "test:fast:parallel": "SERVER_TEST_SCRIPT=test:inner:run:fast:parallel pnpm -w test:server:docker",
    "test:inner:run": "DATABASE_ADAPTER=${DATABASE_ADAPTER:-direct} NODE_ENV=test DISABLE_REQUEST_LOGGING=${DISABLE_REQUEST_LOGGING:-1} MOCK_CLOUD_STORAGE=${MOCK_CLOUD_STORAGE:-1} BCRYPT_SALT_ROUNDS=${BCRYPT_SALT_ROUNDS:-8} bun test --timeout 15000 --max-concurrency 1 src/__tests__",
    "test:inner:run:fast:parallel": "DATABASE_ADAPTER=${DATABASE_ADAPTER:-direct} NODE_ENV=test ENABLE_FAST_BCRYPT_FOR_TESTS=1 BCRYPT_SALT_ROUNDS=4 DISABLE_REQUEST_LOGGING=1 MOCK_CLOUD_STORAGE=1 bun test --timeout 15000 src/__tests__",
    "dev": "bun run src/index.bun.ts",
    "build": "tsc -p tsconfig.json",
    "start": "bun run dist/index.bun.js",
    "startdev": "CLIENT_URL=http://localhost:5173 bun run dist/index.bun.js",
    "startbuilt": "CLIENT_URL=http://localhost:4173 bun run dist/index.bun.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:test": "DATABASE_ADAPTER=${DATABASE_ADAPTER:-direct} prisma migrate deploy",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/zod-validator": "^0.7.6",
    "@markstagram/shared-types": "file:../packages/shared-types",
    "@prisma/adapter-neon": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "firebase-admin": "^11.10.1",
    "hono": "^4.12.2",
    "jose": "^6.1.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@prisma/adapter-pg": "^7.4.1",
    "pg": "^8.19.0",
    "prisma": "^7.4.0",
    "typescript": "^5.7.0"
  },
  "description": ""
}
